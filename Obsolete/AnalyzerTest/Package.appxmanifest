  xmlns:uap="http://schemas.microsoft.com/appx/manifest/uap/windows10"
  IgnorableNamespaces="uap mp">

  <Identity
    Name="ef38fe23-261f-4e1b-8bfe-fd2a99174269"
    Publisher="CN=tonuv"
    Version="1.0.0.0" />

  <mp:PhoneIdentity PhoneProductId="ef38fe23-261f-4e1b-8bfe-fd2a99174269" PhonePublisherId="00000000-0000-0000-0000-000000000000"/>

  <Properties>
