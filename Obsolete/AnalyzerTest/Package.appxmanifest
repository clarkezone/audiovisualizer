    </Application>
  </Applications>
  <Extensions>
    <Extension Category="windows.activatableClass.inProcessServer">
      <InProcessServer>
        <Path>AudioVisualizer.dll</Path>
        <ActivatableClass ActivatableClassId="AudioAnalyzer.MftAnalyzer" ThreadingModel="both" />
